<h1>Feedback list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Type_id</th>
            <th>Status</th>
            <th>Question</th>
            <th>Answerproced</th>
            <th>Answer</th>
            <th>Modified_by</th>
            <th>Modified_at</th>
            <th>Created_at</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('feedback_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.typeid }}</td>
            <td>{{ entity.status }}</td>
            <td>{{ entity.question }}</td>
            <td>{{ entity.answerproced }}</td>
            <td>{{ entity.answer }}</td>
            <td>{{ entity.modifiedby }}</td>
            <td>{% if entity.modifiedat %}{{ entity.modifiedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.createdat %}{{ entity.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('feedback_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('feedback_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('feedback_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
