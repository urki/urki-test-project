<h1>Feedback</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Type_id</th>
            <td>{{ entity.typeid }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Question</th>
            <td>{{ entity.question }}</td>
        </tr>
        <tr>
            <th>Answerproced</th>
            <td>{{ entity.answerproced }}</td>
        </tr>
        <tr>
            <th>Answer</th>
            <td>{{ entity.answer }}</td>
        </tr>
        <tr>
            <th>Modified_by</th>
            <td>{{ entity.modifiedby }}</td>
        </tr>
        <tr>
            <th>Modified_at</th>
            <td>{{ entity.modifiedat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('feedback') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('feedback_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('feedback_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
